{
    "name": "sonata-project/user-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataUserBundle",
    "keywords": ["user", "sonata", "google authenticator"],
    "homepage": "http://sonata-project.org/bundles/user",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataUserBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0",
        "symfony/http-foundation": "@dev",
        "symfony/form": "@dev",
        "symfony/security": "@dev",
        "symfony/console": "@dev",
        "sonata-project/core-bundle": "^3.0",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/datagrid-bundle": "@dev",
        "friendsofsymfony/user-bundle": "@dev",
        "sonata-project/doctrine-extensions": "@dev",
        "sonata-project/easy-extends-bundle": "@dev",
        "sonata-project/google-authenticator": "@dev",
        "friendsofsymfony/rest-bundle": "@dev",
        "jms/serializer-bundle": "@dev",
        "nelmio/api-doc-bundle": "@dev"
    },
    "require-dev": {
        "sonata-project/seo-bundle": "^1.1",
        "doctrine/orm": "^2.0",
        "symfony/phpunit-bridge": "^2.8 || ^3.0"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/seo-bundle": "^1.1"
    },
    "conflict": {
        "jms/serializer": "<0.13"
    },
    "autoload": {
        "psr-4": { "Sonata\\UserBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
